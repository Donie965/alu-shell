#!/usr/bin/env bash
#!/bin/bash

# Process name
PROCESS_NAME="7-highlander"

# Killing the process
pkill "$PROCESS_NAME"

# Check if the process was killed successfully
if [ $? -eq 0 ]; then
    echo "Process '$PROCESS_NAME' has been killed."
else
    echo "Failed to kill process '$PROCESS_NAME'."
fi
